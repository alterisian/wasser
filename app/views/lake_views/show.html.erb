<%- model_class = LakeView -%>
<div class="page-header">
  <h1><%=t '.title', :default => LakeView %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:tl_lat) %></dt>
      <dd><pre class="prettyprint"><%= @lake_view.tl_lat %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:tl_lng) %></dt>
      <dd><pre class="prettyprint"><%= @lake_view.tl_lng %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:br_lat) %></dt>
      <dd><pre class="prettyprint"><%= @lake_view.br_lat %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:br_lng) %></dt>
      <dd><pre class="prettyprint"><%= @lake_view.br_lng %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              lake_views_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_lake_view_path(@lake_view), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              lake_view_path(@lake_view),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

