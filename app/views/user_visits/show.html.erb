<%- model_class = UserVisit -%>
<div class="page-header">
  <h1><%=t '.title', :default => UserVisit %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:description) %></dt>
      <dd><pre class="prettyprint"><%= @user_visit.description %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:public) %></dt>
      <dd><pre class="prettyprint"><%= @user_visit.public %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              user_visits_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_user_visit_path(@user_visit), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              user_visit_path(@user_visit),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

